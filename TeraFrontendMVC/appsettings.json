{
  "BackendSettings": {
    "BaseUrl": "http://web/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthenticatedRoutes": [
    "/Results",
    "/Account/UserProfile",
    "/Account/ChangePassword",
    "/Account/RegisterForAdmin"
  ],
  "UnauthenticatedRoutes": [
    "/Account/Login",
    "/Account/Register"
  ],
  "RoleRestrictedRoutes": {
    "/Account/RegisterForAdmin": [ "Admin" ]
  }
}
