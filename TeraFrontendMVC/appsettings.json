{
  "BackendSettings": {
    "BaseUrl": "http://web/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiUrls": {
    "Usuarios": "http://web/api/Usuarios",
    "Resultados": "http://web/api/Resultados",
    "Region": "http://web/api/Region"
  },
  "AuthenticatedRoutes": [
    "/Results",
    "/Account/UserProfile",
    "/Account/ChangePassword",
    "/Account/RegisterForAdmin",
    "/Account/ListUsersForAdmin",
    "/Account/EditUser"
  ],
  "UnauthenticatedRoutes": [
    "/Account/Login",
    "/Account/Register"
  ],
  "RoleRestrictedRoutes": {
    "/Account/RegisterForAdmin": [ "Admin" ],
    "/Account/ListUsersForAdmin": [ "Admin" ]
  }
}
